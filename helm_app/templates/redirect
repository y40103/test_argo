apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: login-redirect
  namespace: dev
  annotations:
    nginx.ingress.kubernetes.io/rewrite-target: /
    nginx.ingress.kubernetes.io/permanent-redirect: "https://www.google.com"
spec:
  ingressClassName: nginx
  rules:
  - host: mingdetest.com
    http:
      paths:
      - path: /minio/login
        pathType: Prefix
        backend:
          service:
            name: svc-minio-ui
            port:
              number: 9001

